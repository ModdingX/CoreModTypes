{
  "compilerOptions": {
    "target": "ES5",
    "moduleResolution": "classic",
    "lib": ["ES5"],
    "alwaysStrict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
    "module": "CommonJS"
  }
}
